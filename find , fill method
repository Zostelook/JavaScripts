const myArray = ["hello","caat", "dogy","lion", "cat"];

function isLength3(string){
    return string.length===3;
}

const ans = myArray.find(string=>string.length===3);
console.log(ans);


const numbers = [2,4,6,8,11];

const anss = numbers.every(number=>number%2===0);
console.log(anss);


//how to create an array
const myyArray = new Array(10).fill("true");
console.log(myyArray);

//fill(vlaue ,startIndex, endIndex);
const arr = [3,4,5,6,7,8,9,1];
arr.fill(0,3,6);
console.log(arr);
